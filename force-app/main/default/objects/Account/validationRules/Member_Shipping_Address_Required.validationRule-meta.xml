<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Member_Shipping_Address_Required</fullName>
    <active>false</active>
    <description>Shipping address required</description>
    <errorConditionFormula>AND(ShippingStreet = NULL,  $User.Alias != &#39;iserv&#39;,  IsPersonAccount = true)</errorConditionFormula>
    <errorMessage>Shipping Street address is required.</errorMessage>
</ValidationRule>
